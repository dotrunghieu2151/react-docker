{
  "presets": [
    [
      "@babel/preset-env",
      {
        "bugfixes": true
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime",  {
        "regenerator": true
      }],
    ["@babel/plugin-proposal-class-properties", { "loose": false }]
  ],
  "ignore": ["node_modules", "build"]
}